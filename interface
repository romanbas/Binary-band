    private void DepositOnMemoryActionPerformed(java.awt.event.ActionEvent evt) {                                                
       
        String Address = Address_textfield.getText();
        int Int_address = Integer.parseInt(Address);
        String value = value_txtbox.getText();
       // String Str_value = Integer.toBinaryString(Integer.parseInt(value)); 

        char[] Vlaue_textfield_before_Convert = new char[value.length()];
        char[] Bit_value = new char[value.length()];
        Vlaue_textfield_before_Convert = value.toCharArray();

        for (int i = 0, j = Bit_value.length - 1; i < Bit_value.length; i++, j--) {
            Bit_value[i] = Vlaue_textfield_before_Convert[j];

        }

        int Bit_value_INT[] = new int[Bit_value.length];

        for (int i = 0; i < Bit_value.length; i++) {
            Bit_value_INT[i] = Bit_value[i] - '0';

        }
        
        instruct.SM.writeMemoryInstruction(Int_address, Bit_value_INT);
        instruct.decodeInstruction();
        instruct.EffectiveAddress();
        if(instruct.getdecimalvalue(instruct.Opcode)>0 && instruct.getdecimalvalue(instruct.Opcode)<65){
         Address_textfield.setText( Integer.toString(instruct.effectiveAdd));}
    }                                               

    private void IPLActionPerformed(java.awt.event.ActionEvent evt) {                                    
        instruct.SM.setZero();
    }                                   

    private void Address_textfieldActionPerformed(java.awt.event.ActionEvent evt) {                                                  

    }                                                 

