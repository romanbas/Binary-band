    private void jButton10ActionPerformed(java.awt.event.ActionEvent evt) {                                          
// this function calculate the value of program counter by converting the decimal value to binary then display the value in the radio button

        String PC_ValueTextfield = pc_vlaue_textfield.getText();
        pc_value_lable.setText(PC_ValueTextfield);
        String PC_value = Integer.toBinaryString(Integer.parseInt(PC_ValueTextfield));

        char[] Bit_pcConvert = new char[PC_value.length()];
        char[] Bit_pc = new char[PC_value.length()];
        Bit_pcConvert = PC_value.toCharArray();
        for (int i = 0, j = Bit_pc.length - 1; i < Bit_pc.length; i++, j--) {
            Bit_pc[i] = Bit_pcConvert[j];
        }
        int Bit_pc_INT[] = new int[Bit_pc.length];
        for (int i = 0; i < Bit_pc.length; i++) {
            Bit_pc_INT[i] = Bit_pc[i] - '0';
        }
        instruct.cp.setPC(Bit_pc_INT);

        for (int i = 0; i < Bit_pc.length; i++) {
            //System.out.println("Bit_pc"+i+Bit_pc[i]);
            if (Bit_pc[i] == '1') {
                pc_Valu_set[i] = true;

            } else {
                pc_Valu_set[i] = false;
            }
        }

        if (pc_Valu_set[0] == true) {
            pc_11.setSelected(true);
        } else {
            pc_11.setSelected(false);
        }

        if (pc_Valu_set[1] == true) {
            pc_10.setSelected(true);
        } else {
            pc_10.setSelected(false);
        }

        if (pc_Valu_set[2] == true) {
            pc_9.setSelected(true);
        } else {
            pc_9.setSelected(false);
        }

        if (pc_Valu_set[3] == true) {
            pc_8.setSelected(true);
        } else {
            pc_8.setSelected(false);
        }
        if (pc_Valu_set[4] == true) {
            pc_7.setSelected(true);
        } else {
            pc_7.setSelected(false);
        }
        if (pc_Valu_set[5] == true) {
            pc_6.setSelected(true);
        } else {
            pc_6.setSelected(false);
        }
        if (pc_Valu_set[6] == true) {
            pc_5.setSelected(true);
        } else {
            pc_5.setSelected(false);
        }
        if (pc_Valu_set[7] == true) {
            pc_4.setSelected(true);
        } else {
            pc_4.setSelected(false);
        }
        if (pc_Valu_set[8] == true) {
            pc_3.setSelected(true);
        } else {
            pc_3.setSelected(false);
        }
        if (pc_Valu_set[9] == true) {
            pc_2.setSelected(true);
        } else {
            pc_2.setSelected(false);
        }
        if (pc_Valu_set[10] == true) {
            pc_1.setSelected(true);
        } else {
            pc_1.setSelected(false);
        }
        if (pc_Valu_set[11] == true) {
            pc_0.setSelected(true);
        } else {
            pc_0.setSelected(false);
        }
        int address = instruct.getInsAdd();
        String Address_text = Integer.toString(address);
        Address_textfield.setText(Address_text);


    }                                         

