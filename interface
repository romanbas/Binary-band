    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
// this function calculate the value of register 0 by converting the decimal value to binary then display the value in the radio button 
        String R0_ValueTextfield = R0_Textfield.getText();
        R0_value_lable1.setText(R0_ValueTextfield);
        String R0_value = Integer.toBinaryString(Integer.parseInt(R0_ValueTextfield));

        char[] Bit_R0_Convert = new char[R0_value.length()];
        char[] Bit_R0 = new char[R0_value.length()];
        Bit_R0_Convert = R0_value.toCharArray();

        for (int i = 0, j = Bit_R0.length - 1; i < Bit_R0.length; i++, j--) {
            Bit_R0[i] = Bit_R0_Convert[j];
        }
        int Bit_R0_INT[] = new int[Bit_R0.length];
        for (int i = 0; i < Bit_R0.length; i++) {
            Bit_R0_INT[i] = Bit_R0[i] - '0';
        }
        instruct.cp.setR0(Bit_R0_INT);

        for (int i = 0; i < R0_Valu_set.length; i++) {
            R0_Valu_set[i] = false;
        }
        for (int i = 0; i < Bit_R0.length; i++) {
            //System.out.println("Bit_pc"+i+Bit_pc[i]);
