    private void Run_single_stepActionPerformed(java.awt.event.ActionEvent evt) {                                                
// execute the instruction 
        instruct.setPC();
        instruct.decodecacheline();
        //instruct.EffectiveAddress();
        //instruct.excuteInstruction();
        int indexofR = instruct.R[0] + instruct.R[1] * 2;
// reset the R0 value and display the new value 
        if (indexofR == 0) {
            R0_0.setSelected(false);
            R0_1.setSelected(false);
            R0_2.setSelected(false);
            R0_3.setSelected(false);
            R0_4.setSelected(false);
            R0_5.setSelected(false);
            R0_6.setSelected(false);
            R0_7.setSelected(false);
            R0_8.setSelected(false);
            R0_9.setSelected(false);
            R0_10.setSelected(false);
            R0_11.setSelected(false);
            R0_12.setSelected(false);
            R0_13.setSelected(false);
            R0_14.setSelected(false);
            R0_15.setSelected(false);
            int[] R0_data = instruct.cp.R0.data;
            // reset the array value 
            for (int i = 0; i < R0_Valu_set.length; i++) {
                R0_Valu_set[i] = false;
            }

// change the array to T if the binary value is 1 and to F if the value is 0
            for (int i = 0; i < R0_data.length; i++) {

                if (R0_data[i] == 1) {
                    R0_Valu_set[i] = true;

                } else {
                    R0_Valu_set[i] = false;
                }
            }

            //************************************display the radio Button *****************************
            if (R0_Valu_set[0] == true) {
                R0_0.setSelected(true);

            } else {
                R0_0.setSelected(false);
            }

            if (R0_Valu_set[1] == true) {
                R0_1.setSelected(true);
            } else {
                R0_1.setSelected(false);
            }

            if (R0_Valu_set[2] == true) {
                R0_2.setSelected(true);
            } else {
                R0_2.setSelected(false);
            }

            if (R0_Valu_set[3] == true) {
                R0_3.setSelected(true);
            } else {
                R0_3.setSelected(false);
            }

            if (R0_Valu_set[4] == true) {
                R0_4.setSelected(true);
            } else {
                R0_4.setSelected(false);
            }
            if (R0_Valu_set[5] == true) {
                R0_5.setSelected(true);
            } else {
                R0_5.setSelected(false);
            }
            if (R0_Valu_set[6] == true) {
                R0_6.setSelected(true);
            } else {
                R0_6.setSelected(false);
            }
