    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // this function calculate the value of index register 3 by converting the decimal value to binary then display the value in the radio button

        String I3_ValueTextfield = I3_Textfield.getText();// get the value from the text field 
        I3_value_lable.setText(I3_ValueTextfield);
        String I3_value = Integer.toBinaryString(Integer.parseInt(I3_ValueTextfield));// convert to binary string 

        char[] Bit_I3_Convert = new char[I3_value.length()];
        char[] Bit_I3 = new char[I3_value.length()];
        Bit_I3_Convert = I3_value.toCharArray();
        // convert to reverse order 
        for (int i = 0, j = Bit_I3.length - 1; i < Bit_I3.length; i++, j--) {
            Bit_I3[i] = Bit_I3_Convert[j];
        }
        int Bit_I3_INT[] = new int[Bit_I3.length];
        // convert the char array to int array 

        for (int i = 0; i < Bit_I3.length; i++) {
            Bit_I3_INT[i] = Bit_I3[i] - '0';
        }
        instruct.cp.setX3(Bit_I3_INT);
// reset the array valut to F 
        for (int i = 0; i < I3_Valu_set.length; i++) {
            I3_Valu_set[i] = false;
        }
        // change the array to T if the binary value is 1 and to F if the value is 0
        for (int i = 0; i < Bit_I3.length; i++) {

            if (Bit_I3[i] == '1') {
                I3_Valu_set[i] = true;
            } else {
                I3_Valu_set[i] = false;
            }
        }
// display the value in radio button 
        if (I3_Valu_set[0] == true) {
            I3_0.setSelected(true);
        } else {
            I3_0.setSelected(false);
        }

        if (I3_Valu_set[1] == true) {
            I3_1.setSelected(true);
        } else {
            I3_1.setSelected(false);
        }

        if (I3_Valu_set[2] == true) {
            I3_2.setSelected(true);
        } else {
            I3_2.setSelected(false);
        }

        if (I3_Valu_set[3] == true) {
            I3_3.setSelected(true);
        } else {
            I3_3.setSelected(false);
        }

        if (I3_Valu_set[4] == true) {
            I3_4.setSelected(true);
        } else {
            I3_4.setSelected(false);
        }
        if (I3_Valu_set[5] == true) {
            I3_5.setSelected(true);
        } else {
            I3_5.setSelected(false);
        }
        if (I3_Valu_set[6] == true) {
            I3_6.setSelected(true);
        } else {
            I3_6.setSelected(false);
        }
        if (I3_Valu_set[7] == true) {
            I3_7.setSelected(true);
        } else {
            I3_7.setSelected(false);
        }
        if (I3_Valu_set[8] == true) {
            I3_8.setSelected(true);
        } else {
            I3_8.setSelected(false);
        }
        if (I3_Valu_set[9] == true) {
            I3_9.setSelected(true);
        } else {
            I3_9.setSelected(false);
        }
        if (I3_Valu_set[10] == true) {
            I3_10.setSelected(true);
        } else {
