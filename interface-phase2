    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // this function calculate the value of index register 2 by converting the decimal value to binary then display the value in the radio button

        String I2_ValueTextfield = I2_Textfield.getText();// get the value from the text field 
        I2_value_lable.setText(I2_ValueTextfield);
        String I2_value = Integer.toBinaryString(Integer.parseInt(I2_ValueTextfield));// convert to binary string 

        char[] Bit_I2_Convert = new char[I2_value.length()];
        char[] Bit_I2 = new char[I2_value.length()];
        Bit_I2_Convert = I2_value.toCharArray();
        // convert to reverse order 
        for (int i = 0, j = Bit_I2.length - 1; i < Bit_I2.length; i++, j--) {
            Bit_I2[i] = Bit_I2_Convert[j];
        }

        int Bit_I2_INT[] = new int[Bit_I2.length];
        // convert the char to int 
        for (int i = 0; i < Bit_I2.length; i++) {
            Bit_I2_INT[i] = Bit_I2[i] - '0';
        }
        instruct.cp.setX2(Bit_I2_INT);
// reset the array value to F 
        for (int i = 0; i < I2_Valu_set.length; i++) {
            I2_Valu_set[i] = false;
        }
        // change the array to T if the binary value is 1 and to F if the value is 0
        for (int i = 0; i < Bit_I2.length; i++) {

            if (Bit_I2[i] == '1') {
                I2_Valu_set[i] = true;
            } else {
