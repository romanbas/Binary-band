    private void jButton12ActionPerformed(java.awt.event.ActionEvent evt) {                                          
      OFF_BUTTON.setText("ON");
      OFF_BUTTON.setBackground(Color.green);
      String newline = "\n";

      
      program1 pro = new program1 ();
        try {
            pro.readfile();
        } catch (IOException ex) {
            Logger.getLogger(GUI_Arch.class.getName()).log(Level.SEVERE, null, ex);
        }
        
        String userInput=engineers_console.getText();
        int userinputinInt= Integer.parseInt(userInput);
        // engineers_console.append(newline);
           int printComp= pro.compare(userinputinInt);
       engineers_console.append(newline+"the Closest number in the array is "+printComp+newline);
        for(int i=0; i<pro.decimalfrMem.length;i++)
        {
        engineers_console.append("decimalfrMem -->"+pro.decimalfrMem[i] + newline);
   engineers_console.append(""+pro.stringofBinary[i]);
          
    engineers_console.append(newline);
        }
        
         
        
        //enter pro.usernumber  pro.userNumber
     
        
        //print:pro.theNumber(int)
        
      
       
    }                                         

