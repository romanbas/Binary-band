    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // this function calculate the value of index register 1 by converting the decimal value to binary then display the value in the radio button
        String I1_ValueTextfield = I1_Textfield.getText();// get the value from the text field 
        I1_value_lable.setText(I1_ValueTextfield);
        String I1_value = Integer.toBinaryString(Integer.parseInt(I1_ValueTextfield));// convert to binary string 

        char[] Bit_I1_Convert = new char[I1_value.length()];
        char[] Bit_I1 = new char[I1_value.length()];
        Bit_I1_Convert = I1_value.toCharArray();
        // convert the binary in to reverse order 
        for (int i = 0, j = Bit_I1.length - 1; i < Bit_I1.length; i++, j--) {
            Bit_I1[i] = Bit_I1_Convert[j];
        }
        int Bit_I0_INT[] = new int[Bit_I1.length];
        // char array  to int array 
        for (int i = 0; i < Bit_I1.length; i++) {
            Bit_I0_INT[i] = Bit_I1[i] - '0';
