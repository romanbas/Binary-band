    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // this function calculate the value of register 1 by converting the decimal value to binary then display the value in the radio button

        String R1_ValueTextfield = R1_Textfield.getText();// get the value from the text field 
        R1_value_lable1.setText(R1_ValueTextfield);
        String R1_value = Integer.toBinaryString(Integer.parseInt(R1_ValueTextfield));// convert to binary string 

        char[] Bit_R1_Convert = new char[R1_value.length()];
        char[] Bit_R1 = new char[R1_value.length()];
        Bit_R1_Convert = R1_value.toCharArray();
// put it in reverse order 
        for (int i = 0, j = Bit_R1.length - 1; i < Bit_R1.length; i++, j--) {
            Bit_R1[i] = Bit_R1_Convert[j];
        }
        // convert into int array 
        int Bit_R1_INT[] = new int[Bit_R1.length];
        for (int i = 0; i < Bit_R1.length; i++) {
            Bit_R1_INT[i] = Bit_R1[i] - '0';
        }
        instruct.cp.setR1(Bit_R1_INT);
        // reset the array value to F 
        for (int i = 0; i < R1_Valu_set.length; i++) {
            R1_Valu_set[i] = false;
        }
        // change the array to T if the binary value is 1 and to F if the value is 0
        for (int i = 0; i < Bit_R1.length; i++) {

            if (Bit_R1[i] == '1') {
                R1_Valu_set[i] = true;

            } else {
                R1_Valu_set[i] = false;
            }
        }
// display the binary value in the radio button 
        if (R1_Valu_set[0] == true) {
            R1_0.setSelected(true);
        } else {
            R1_0.setSelected(false);
        }

        if (R1_Valu_set[1] == true) {
            R1_1.setSelected(true);
        } else {
            R1_1.setSelected(false);
        }

        if (R1_Valu_set[2] == true) {
            R1_2.setSelected(true);
        } else {
            R1_2.setSelected(false);
        }

        if (R1_Valu_set[3] == true) {
            R1_3.setSelected(true);
        } else {
            R1_3.setSelected(false);
        }

        if (R1_Valu_set[4] == true) {
            R1_4.setSelected(true);
        } else {
            R1_4.setSelected(false);
        }
        if (R1_Valu_set[5] == true) {
            R1_5.setSelected(true);
        } else {
            R1_5.setSelected(false);
        }
        if (R1_Valu_set[6] == true) {
