    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
// this function calculate the value of register 2 by converting the decimal value to binary then display the value in the radio button
        String R2_ValueTextfield = R2_Textfield.getText();// get the value from the text field 
        R2_value_lable.setText(R2_ValueTextfield);
        String R2_value = Integer.toBinaryString(Integer.parseInt(R2_ValueTextfield));// convert to binary string 

        char[] Bit_R2_Convert = new char[R2_value.length()];
        char[] Bit_R2 = new char[R2_value.length()];
        Bit_R2_Convert = R2_value.toCharArray();
        // put the data in reverse order 
        for (int i = 0, j = Bit_R2.length - 1; i < Bit_R2.length; i++, j--) {
            Bit_R2[i] = Bit_R2_Convert[j];
        }
        int Bit_R2_INT[] = new int[Bit_R2.length];
        // convert to int array 
        for (int i = 0; i < Bit_R2.length; i++) {
            Bit_R2_INT[i] = Bit_R2[i] - '0';
        }
        instruct.cp.setR2(Bit_R2_INT);

        // reset the array value 
        for (int i = 0; i < R2_Valu_set.length; i++) {
            R2_Valu_set[i] = false;
        }
        // change the array to T if the binary value is 1 and to F if the value is 0
        for (int i = 0; i < Bit_R2.length; i++) {

            if (Bit_R2[i] == '1') {
                R2_Valu_set[i] = true;
