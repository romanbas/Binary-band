                I3_13.setSelected(true);
            } else {
                I3_13.setSelected(false);
            }
            if (I3_Valu_set[14] == true) {
                I3_14.setSelected(true);
            } else {
                I3_14.setSelected(false);
            }
            if (I3_Valu_set[15] == true) {
                I3_15.setSelected(true);
            } else {
                I3_15.setSelected(false);
            }
        }

