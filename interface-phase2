    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
// this function calculate the value of register 0 by converting the decimal value to binary then display the value in the radio button 
        String R0_ValueTextfield = R0_Textfield.getText();// get the value from the text field 
        R0_value_lable1.setText(R0_ValueTextfield);
        String R0_value = Integer.toBinaryString(Integer.parseInt(R0_ValueTextfield));// convert to binary string 

        char[] Bit_R0_Convert = new char[R0_value.length()];
        char[] Bit_R0 = new char[R0_value.length()];
        Bit_R0_Convert = R0_value.toCharArray();
        // put it in reverse order 
        for (int i = 0, j = Bit_R0.length - 1; i < Bit_R0.length; i++, j--) {
            Bit_R0[i] = Bit_R0_Convert[j];
        }
        // convert to int array 
        int Bit_R0_INT[] = new int[Bit_R0.length];
        for (int i = 0; i < Bit_R0.length; i++) {
            Bit_R0_INT[i] = Bit_R0[i] - '0';
        }
        instruct.cp.setR0(Bit_R0_INT);
// reset the array value 
        for (int i = 0; i < R0_Valu_set.length; i++) {
            R0_Valu_set[i] = false;
        }

        // change the array to T if the binary value is 1 and to F if the value is 0
        for (int i = 0; i < Bit_R0.length; i++) {

            if (Bit_R0[i] == '1') {
                R0_Valu_set[i] = true;

            } else {
                R0_Valu_set[i] = false;
            }
        }
// display the binary value in the radio button 
        if (R0_Valu_set[0] == true) {
            R0_0.setSelected(true);
        } else {
            R0_0.setSelected(false);
        }

        if (R0_Valu_set[1] == true) {
            R0_1.setSelected(true);
        } else {
            R0_1.setSelected(false);
        }

        if (R0_Valu_set[2] == true) {
            R0_2.setSelected(true);
        } else {
            R0_2.setSelected(false);
        }

        if (R0_Valu_set[3] == true) {
            R0_3.setSelected(true);
        } else {
            R0_3.setSelected(false);
        }

        if (R0_Valu_set[4] == true) {
            R0_4.setSelected(true);
        } else {
            R0_4.setSelected(false);
        }
        if (R0_Valu_set[5] == true) {
            R0_5.setSelected(true);
        } else {
            R0_5.setSelected(false);
        }
        if (R0_Valu_set[6] == true) {
            R0_6.setSelected(true);
        } else {
            R0_6.setSelected(false);
        }
        if (R0_Valu_set[7] == true) {
            R0_7.setSelected(true);
        } else {
            R0_7.setSelected(false);
        }
        if (R0_Valu_set[8] == true) {
            R0_8.setSelected(true);
        } else {
            R0_8.setSelected(false);
        }
        if (R0_Valu_set[9] == true) {
